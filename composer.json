{
    "name": "dazz/oauth2-server-httpfoundation-webtest",
    "description": "A bridge for webtesting oauth2-server-php with bshaffer/oauth2-server-httpfoundation-bridge and silex",
    "keywords": ["oauth", "oauth2", "auth", "httpfoundation", "test", "webtest"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hakin Dazs",
            "email": "hakindazs@gmail.com"
        }
    ],
    "homepage": "http://github.com/dazz/oauth2-server-httpfoundation-webtest",
    "require": {
        "php": ">=5.3.9",
        "bshaffer/oauth2-server-httpfoundation-bridge": "dev-master"
    },
    "require-dev": {
        "symfony/http-foundation": ">=2.3,<2.5-dev",
        "symfony/browser-kit": ">=2.3,<2.5-dev",
        "symfony/css-selector": ">=2.3,<2.5-dev",
        "symfony/dom-crawler": ">=2.3,<2.5-dev"
    },
    "autoload": {
        "psr-0": {
            "OAuth2\\HttpFoundationWebTest": "src/"
        }
    },
    "suggest": {
        "bshaffer/oauth2-server-php": "dev-develop",
        "silex/silex": "dev-master"
    }
}
